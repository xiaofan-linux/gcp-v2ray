{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": "{PORT}",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "{ID}",
            "alterId": {ALTERID}
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "block"
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "outboundTag": "block",
        "protocol": [
          "bittorrent"
        ]
      }
    ]
  },
  "dns": {
    "server": [
      "8.8.8.8",
      "8.8.4.4",
      "localhost"
    ]
  }
}
